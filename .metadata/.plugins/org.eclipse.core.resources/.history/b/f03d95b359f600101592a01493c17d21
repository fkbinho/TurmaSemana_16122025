package laco_repeticao;

public class LacoFor {

	public static void main(String[] args) {
		
		for(int i = 1; i <= 10; i++) {
			System.out.print(i + " ");
		}
		
		System.out.println("\n---------------");
		
		for(int i = 10; i > 0; i--) {
			System.out.print(i + " ");
		}
		
		System.out.println("\n---------------");
		
		String nome = "Tadeu";
		// t a d e u
		
		System.out.println(nome.length());
		
		for(int i = 0; i <= nome.length();) {
			
		}
	}

}
